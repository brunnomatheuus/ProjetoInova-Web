{
  "name": "light-bootstrap-dashboard-react",
  "version": "1.2.0",
  "private": true,
  "dependencies": {
    "@amcharts/amcharts4": "^4.0.0",
    "@material-ui/core": "^3.4.0",
    "axios": "^0.18.0",
    "bootstrap": "3.3.7",
    "chartist": "0.10.1",
    "cpf_cnpj": "^0.2.0",
    "final-form": "^4.10.0",
    "history": "^4.7.2",
    "node-sass": "4.6.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2",
    "react": "16.3.0",
    "react-bootstrap": "0.32.1",
    "react-chartist": "0.13.1",
    "react-d3-speedometer": "^0.4.2",
    "react-dom": "16.2.0",
    "react-final-form": "^3.6.7",
    "react-google-maps": "9.4.5",
    "react-helmet": "^5.2.0",
    "react-input-mask": "^2.0.4",
    "react-masked-text": "^0.3.2",
    "react-modal": "^3.6.1",
    "react-native-masked-text": "^1.9.2",
    "react-notification-system": "0.2.17",
    "react-responsive-modal": "^3.5.1",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.1",
    "react-select": "^2.1.1",
    "zingchart-react": "^1.0.6"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
